generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  Created  DateTime @default(now()) @map("created_at")
  Username String
  Aadhar   String @unique
}
