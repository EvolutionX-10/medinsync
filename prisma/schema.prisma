generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  created  DateTime @default(now()) @map("created_at")
  username String
  aadhaar  String   @unique
}
